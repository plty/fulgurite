---
import { type Lang } from "$components/editor/lang-support";
import { StaticBlox } from "$components/StaticBlox";

interface Props {
  content: string;
  language: Lang;
}

const { content, language } = Astro.props;
---

<dev class="no-prose text-sm">
  <StaticBlox code={content} lang={language} lineGroup={{}} client:load />
</dev>
